            <enabled>false</enabled>
        </fire>
        <landscape_removed>
            <enabled>false</enabled>
            <includeHarvest>false</includeHarvest>
            <includeNatural>true</includeNatural>
        </landscape_removed>
        <!-- ABE (management) -->
        <abeStand>
            <enabled>false</enabled>
        </abeStand>
        <abeStandDetail>
            <enabled>false</enabled>
        </abeStandDetail>
        <abeUnit>
            <enabled>false</enabled>
        </abeUnit>
        <abeStandRemoval>
            <enabled>false</enabled>
        </abeStandRemoval>
        <!-- Outputs related to SVD -->
        <svdgpp>
            <enabled>false</enabled>
        </svdgpp>
        <svdindicator>
            <enabled>false</enabled>
            <indicators>
                <shannonIndex>false</shannonIndex>
                <abovegroundCarbon>false</abovegroundCarbon>
                <totalCarbon>false</totalCarbon>
                <volume>false</volume>
                <crownCover>false</crownCover>
                <LAI>false</LAI>
                <basalArea>false</basalArea>
                <stemDensity>false</stemDensity>
                <saplingDensity>false</saplingDensity>
                <speciesProportions>false</speciesProportions>
                <disturbanceHistory>false</disturbanceHistory>
            </indicators>
        </svdindicator>
        <svdstate>
            <enabled>false</enabled>
        </svdstate>
        <svduniquestate>
            <enabled>false</enabled>
            <condition></condition>
        </svduniquestate>
        <devstage>
            <!-- defines the cell size in meters. The value need to align with the 100m / 10m cells. 
		Possible values are therfore: 10, 20, 50, 100 -->
            <cellsize>50</cellsize>
            <enabled>false</enabled>
            <!-- The handling javascript object. See iland-model.org/Outputs+devstage for details -->
            <handler>devStage</handler>
            <!-- Filter function (variable: year) -->
            <filter>mod(year,10)=0</filter>
            <condition>mod(year,10)=0</condition>
        </devstage>
        <ecoviz>
            <enabled>false</enabled>
            <condition />
            <fileName />
        </ecoviz>
    </output>
    <modules>
        <!-- section for "plugins" add a section for each plugin and enable/disable with 
	   the "enabled" switch. 
	   https://iland-model.org/disturbance+modules -->

        <wind>
            <enabled>true</enabled>
            <!-- table name of the species parameter table -->
            <speciesParameter>wind</speciesParameter>
            <!-- yes/no/auto: soil is frozen/not frozen/depends on soil temperature -->
            <soilFreezeMode>auto</soilFreezeMode>
            <!-- true: explicit trigger by time event, false: drawn randomly -->
            <triggeredByTimeEvent>true</triggeredByTimeEvent>
            <!-- duration of a single model iteration  -->
            <durationPerIteration>10</durationPerIteration>
            <!-- variation of (above canopy) wind speed between iterations. Given as fraction, e.g.
            0.05 -> speed +- (max.)5% -->
            <gustModifier>0.1</gustModifier>
            <!-- local windspeed variation (per resource unit) given as multiplier -->
            <topoModifier>1</topoModifier>
            <!-- variation in winddirection for fetch calculation; actual wind direction =
            maindireciton + rnd(-x, +x) -->
            <directionVariation>30</directionVariation>
            <!-- wind direction (°) 0: N, 90: E, 180: S, 270: W -->
            <direction>0</direction>
            <!-- day of the storm (0..365) -->
            <dayOfYear>0</dayOfYear>
            <!-- wind speed (m/s) 10m above the canopy -->
            <speed>0</speed>
            <!-- duration of the wind event in minutes -->
            <duration>720</duration>
            <topoGridFile>gis/wind_scale.asc</topoGridFile>
            <!-- constant ratio between the maximum turning moments at the stand edge and conditions
            well inside the forest (default: 5) -->
            <factorEdge>3</factorEdge>
            <!-- minimum height difference of height-grid pixels to be detected as edges (default is
            10m) -->
            <edgeDetectionThreshold>10</edgeDetectionThreshold>
            <!-- determines if topo-modifier is added multiplicatively or additively.
            "multiplicative" or "additive" -->
            <topexModifierType>additive</topexModifierType>
            <!--  formula for the transfer function LRI to Hegyi competition index -->
            <LRITransferFunction>max(min(3.733-6.467*LRI, 3.41),3)</LRITransferFunction>
            <!-- equation determining the probability that a detected edge is further
            processed(variable: age of the edge) -->
            <edgeProbability>polygon(x,0,0,20,0.5)</edgeProbability>
            <!-- value for the age of edges that is set after the landscape is initialized -->
            <edgeAgeBaseValue>20</edgeAgeBaseValue>
            <!--  gives the probability that a cell (10x10m) acts as start point of wind damage
            (without being an edge cell). A thus "flagged" cell acts as a starting point during the
            whole wind event. -->
            <edgeBackgroundProbability>0.1</edgeBackgroundProbability>
            <!-- event handler called before/after wind event -->
            <onBeforeWind />
            <onAfterWind></onAfterWind>
        </wind>

        <bite>
            <enabled>false</enabled>
        </bite>
    </modules>
    <user>
        <!-- user defined settings (which can be accessed via Javascript) -->
        <repUsed>1</repUsed>
        <gcmUsed>whatever</gcmUsed>
    </user>
</project>